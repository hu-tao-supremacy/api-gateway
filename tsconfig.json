{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@google/*": [
        "src/graphql/google/protobuf/*"
      ],
      "@onepass/inputs/*": [
        "src/inputs/*"
      ],
      "@onepass/account/*": [
        "src/proxy/account/*"
      ],
      "@onepass/facility/*": [
        "src/proxy/facility/*"
      ],
      "@onepass/organizer/*": [
        "src/proxy/organizer/*"
      ],
      "@onepass/participant/*": [
        "src/proxy/participant/*"
      ],
      "@onepass/graphql/*": [
        "src/graphql/hts/*"
      ],
      "@onepass/api/*": [
        "src/apis/hts/*"
      ],
      "@onepass/entities": [
        "src/entities"
      ],
      "@onepass/adapters": [
        "src/adapters"
      ]
    }
  }
}