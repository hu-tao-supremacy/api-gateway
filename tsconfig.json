{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@google/*": [
        "src/graphql/google/protobuf/*"
      ],
      "@onepass/inputs/*": [
        "src/inputs/*"
      ],
      "@onepass/account": [
        "src/proxy/account/account.service.ts"
      ],
      "@onepass/facility": [
        "src/proxy/facility/facility.service.ts"
      ],
      "@onepass/organizer": [
        "src/proxy/organizer/organizer.service.ts"
      ],
      "@onepass/participant": [
        "src/proxy/participant/participant.service.ts"
      ],
      "@onepass/graphql/*": [
        "src/graphql/hts/*"
      ],
      "@onepass/api/*": [
        "src/apis/hts/*"
      ],
      "@onepass/entities": [
        "src/entities"
      ],
      "@onepass/adapters": [
        "src/adapters"
      ]
    }
  }
}